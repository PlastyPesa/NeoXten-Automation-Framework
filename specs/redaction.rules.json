{
  "description": "Redaction rules applied to logs, events, errors, network before writing case outputs",
  "token_patterns": [
    { "pattern": "sk-[a-zA-Z0-9]{20,}", "replacement": "[REDACTED_API_KEY]" },
    { "pattern": "ghp_[a-zA-Z0-9]{36,}", "replacement": "[REDACTED_GITHUB_TOKEN]" },
    { "pattern": "Bearer\\s+[a-zA-Z0-9._-]+", "replacement": "Bearer [REDACTED]" },
    { "pattern": "password[\"']?\\s*[:=]\\s*[\"']?[^\"'\\s]+", "replacement": "password=[REDACTED]" },
    { "pattern": "api_key[\"']?\\s*[:=]\\s*[\"']?[^\"'\\s]+", "replacement": "api_key=[REDACTED]" },
    { "pattern": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}", "replacement": "[REDACTED_UUID]" }
  ],
  "network_strip": {
    "query_params": ["token", "key", "api_key", "apikey", "auth", "session", "cookie"],
    "headers": ["authorization", "x-api-key", "x-auth-token", "cookie"]
  }
}
