{
  "name": "neoxten-automation-framework",
  "version": "2.1.0",
  "description": "Session-based execution environment for automated UI testing, observation, and evidence production",
  "type": "module",
  "bin": {
    "neoxten": "./dist/cli/index.js",
    "nx": "./dist/cli/index.js"
  },
  "main": "./dist/core/orchestrator.js",
  "scripts": {
    "postinstall": "node node_modules/playwright-core/cli.js install chromium",
    "build": "npx tsc",
    "dev": "tsc --watch",
    "run": "node dist/cli/index.js run",
    "gate": "node dist/cli/index.js gate --preset nemyo",
    "test": "node dist/cli/index.js run --config ./neoxten.yaml",
    "test:enoent": "node dist/__tests__/spawn-enoent.test.js",
    "test:paths": "node dist/__tests__/path-resolution.test.js",
    "gen-sample-pack": "node dist/tools/gen-sample-pack.js",
    "test:packs": "node dist/__tests__/packs-ingest.test.js",
    "test:evidence-chain": "node dist/factory/__tests__/evidence-chain.test.js",
    "test:spec-schema": "node dist/factory/__tests__/spec-schema.test.js",
    "test:run-state": "node dist/factory/__tests__/run-state.test.js",
    "test:worker-contract": "node dist/factory/__tests__/worker-contract.test.js",
    "test:gate-registry": "node dist/factory/__tests__/gate-registry.test.js",
    "test:pipeline-config": "node dist/factory/__tests__/pipeline-config.test.js",
    "test:master": "node dist/factory/__tests__/master.test.js",
    "test:consequence-memory": "node dist/factory/__tests__/consequence-memory.test.js",
    "test:workers": "node dist/factory/__tests__/workers.test.js",
    "test:manifest-cli": "node dist/factory/__tests__/manifest-cli.test.js",
    "test:store-pipelines": "node dist/factory/__tests__/store-pipelines.test.js",
    "test:system-validation": "node dist/factory/__tests__/system-validation.test.js",
    "exe:build": "echo EXE build not configured"
  },
  "dependencies": {
    "@playwright/test": "^1.49.0",
    "adm-zip": "^0.5.16",
    "chokidar": "^5.0.0",
    "commander": "^12.1.0",
    "js-yaml": "^4.1.0",
    "playwright": "^1.49.0",
    "uuid": "^10.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  }
}